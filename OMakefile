$(BUILDDIR):
    mkdir -p $(BUILDDIR)

$(BUILDDIR)/js: $(BUILDDIR)
    mkdir -p $(BUILDDIR)/js

$(BUILDDIR)/lib: $(BUILDDIR)
    mkdir -p $(BUILDDIR)/lib

$(BUILDDIR)/ml: $(BUILDDIR)
    mkdir -p $(BUILDDIR)/ml

clean: build
    rmdir $(BUILDDIR)/css
    rmdir $(BUILDDIR)/js
    rmdir $(BUILDDIR)/lib
    rmdir $(BUILDDIR)/ml
